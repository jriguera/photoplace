<interface>
  <requires lib="gtk+" version="2.24"/>
  <!-- interface-naming-policy project-wide -->
  <object class="GtkAdjustment" id="adjustment-jpgzoom">
    <property name="lower">0.000000000</property>
    <property name="upper">1</property>
    <property name="value">0.5</property>
    <property name="step_increment">0.050000000000000003</property>
    <property name="page_increment">0.10000000000000001</property>
    <signal name="value-changed" handler="on_adjustment-jpgzoom_value_changed" swapped="no"/>
  </object>
  <object class="GtkAdjustment" id="adjustment-quality">
    <property name="upper">3</property>
    <property name="value">1</property>
    <property name="step_increment">1</property>
    <property name="page_increment">1</property>
    <signal name="value-changed" handler="on_adjustment-quality_value_changed" swapped="no"/>
  </object>
  <object class="GtkAdjustment" id="adjustment-tdelta">
    <property name="lower">0.01</property>
    <property name="upper">1440</property>
    <property name="value">5</property>
    <property name="step_increment">1</property>
    <property name="page_increment">5</property>
    <signal name="value-changed" handler="on_adjustment-tdelta_value_changed" swapped="no"/>
  </object>
  <object class="GtkAdjustment" id="adjustment-toffset">
    <property name="lower">-3600</property>
    <property name="upper">3600</property>
    <property name="step_increment">1</property>
    <property name="page_increment">60</property>
    <signal name="value-changed" handler="on_adjustment-toffset_value_changed" swapped="no"/>
  </object>
  <object class="GtkAdjustment" id="adjustment-utc">
    <property name="lower">-12</property>
    <property name="upper">12</property>
    <property name="step_increment">1</property>
    <property name="page_increment">2</property>
    <signal name="value-changed" handler="on_adjustment-utc_value_changed" swapped="no"/>
  </object>
  <object class="GtkAboutDialog" id="dialog-about">
    <property name="can_focus">False</property>
    <property name="border_width">5</property>
    <property name="title" translatable="yes" comments="window title">About ...</property>
    <property name="window_position">center-on-parent</property>
    <property name="default_width">600</property>
    <property name="default_height">400</property>
    <property name="destroy_with_parent">True</property>
    <property name="icon">photoplace.png</property>
    <property name="type_hint">dialog</property>
    <property name="program_name">PhotoPlace</property>
